<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baidu.todo.mapper.AccountMapper">
  <select id="getAccount" resultType="com.baidu.todo.entity.Account">
    select user_id, balance from account where id=#{id}
